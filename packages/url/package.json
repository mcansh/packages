{
  "name": "@mcansh/url",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "mcansh/packages",
    "directory": "./packages/url"
  },
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/mcansh"
    },
    {
      "type": "thanks_dev",
      "url": "https://thanks.dev/u/gh/mcansh"
    }
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsdown && attw --pack . --ignore-rules cjs-resolves-to-esm",
    "dev": "tsdown --watch",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown --write .",
    "prepublishOnly": "node --run build",
    "test": "vitest"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "catalog:",
    "@total-typescript/tsconfig": "catalog:",
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "imports": {
    "#*": "./*"
  }
}
