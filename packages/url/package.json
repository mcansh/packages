{
  "name": "@mcansh/url",
  "version": "0.2.2",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mcansh/packages.git",
    "directory": "./packages/http-helmet"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/mcansh"
    }
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "node --run build && node --run attw",
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown --write .",
    "attw": "attw --pack . --ignore-rules cjs-resolves-to-esm",
    "test": "vitest"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "catalog:",
    "@types/node": "catalog:",
    "prettier": "^3.5.3",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "engines": {
    "node": ">=20"
  }
}
