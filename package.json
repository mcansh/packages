{
  "name": "@mcansh/url",
  "version": "0.2.2",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "repository": "mcansh/url",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "node --run build && node --run attw",
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown --write .",
    "attw": "attw --pack . --ignore-rules cjs-resolves-to-esm",
    "test": "node --experimental-strip-types --disable-warning=ExperimentalWarning --test './src/**/*.spec.ts'",
    "test:watch": "node --watch --experimental-strip-types --disable-warning=ExperimentalWarning --test './src/**/*.spec.ts'"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.1",
    "@microsoft/api-extractor": "^7.48.0",
    "@types/node": "^22.10.1",
    "pkg-pr-new": "^0.0.37",
    "prettier": "^3.4.2",
    "tsimp": "^2.0.12",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "engines": {
    "node": ">=20"
  }
}
